#include <iostream>
#include <string>
#include <cctype>
#include <Windows.h>

using namespace std;

bool isAlphanumeric(const string& str) {
    for (char c : str) {
        if (!isalnum(c)) {
            return false;
        }
    }
    return true;
}

int main() {

    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    string input;
    cout << "Введіть рядок: ";
    cin >> input;

    if (isAlphanumeric(input)) {
        cout << "Рядок містить тільки латинські букви та цифри." << endl;
    }
    else {
        cout << "Рядок містить недопустимі символи." << endl;
    }

    return 0;
}
